<Defs>

	<RecipeDef>
		<defName>MakeGraftMalari</defName>
		<label>Сделать прививку от малярии</label>
		<description>Сделать прививку от малярии.3 дозы.</description>
		<jobString>Делает вакцину</jobString>
		<workSpeedStat>DrugCookingSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Chemlab_Medicine_Drink</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<workAmount>1500</workAmount>
		<recipeUsers>
			<li>meditable</li>
			<li>DrugLab</li>
		</recipeUsers>
		<targetCountAdjustment>1</targetCountAdjustment>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>MedicineHerbal</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>MedicalDrink</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Neutroamine</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
		    	<li>MedicalDrink</li>
				<li>Neutroamine</li>
				<li>MedicineHerbal</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<GraftMalari>3</GraftMalari>
		</products>
		<skillRequirements>
			<Medicine>5</Medicine>
		</skillRequirements>
		<workSkill>Medicine</workSkill>
		<researchPrerequisite>PenoxycylineProduction</researchPrerequisite>
	</RecipeDef>

